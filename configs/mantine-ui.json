{
  "name": "mantine-ui",
  "description": "Mantine UI templates and examples library for JavaScript and TypeScript. Use for finding pre-built, production-ready React component examples including headers, heroes, features, cards, forms, authentication pages, and more. 120+ free templates built with Mantine.",
  "base_url": "https://ui.mantine.dev/",
  "start_urls": [
    "https://ui.mantine.dev/",
    "https://ui.mantine.dev/category/headers/",
    "https://ui.mantine.dev/category/hero/",
